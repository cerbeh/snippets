{
  "name": "snippets",
  "version": "1.0.0",
  "description": "Collection of code snippets and poorly thought ideas",
  "main": "apiModule.js",
  "dependencies": {
    "@vue/cli-service": "^4.5.6"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.11.5",
    "@vue/cli-plugin-unit-jest": "^4.5.6",
    "@vue/test-utils": "^1.1.0",
    "babel-core": "^6.26.3",
    "vue": "^2.6.12",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.12"
  },
  "scripts": {
    "test:unit": "npx vue-cli-service test:unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerbeh/snippets.git"
  },
  "author": "Martin Allgood",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cerbeh/snippets/issues"
  },
  "homepage": "https://github.com/cerbeh/snippets#readme",
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest",
    "testMatch": [
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules",
      "/examples"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      ".*\\.(js)$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
